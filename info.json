{
  "id": "plugin.com.pot-app.qwen-ocr",
  "plugin_type": "recognize",
  "icon": "icon.png",
  "display": "Qwen OCR",
  "homepage": "https://github.com/your-username/API-Test",
  "needs": [
    {
      "key": "api_key",
      "display": "API Key",
      "type": "input"
    },
    {
      "key": "base_url",
      "display": "Base URL",
      "type": "input",
      "default": "https://dashscope.aliyuncs.com/compatible-mode/v1"
    },
    {
      "key": "model",
      "display": "Model",
      "type": "input",
      "default": "qwen3-vl-plus"
    },
    {
      "key": "api_format",
      "display": "API 格式",
      "type": "select",
      "options": {
        "openai": "OpenAI 兼容 (推荐)",
        "dashscope": "DashScope 原生"
      }
    },
    {
      "key": "custom_prompt",
      "display": "自定义提示词",
      "type": "input"
    },
    {
      "key": "enable_thinking",
      "display": "思考模式",
      "type": "select",
      "options": {
        "false": "关闭 (默认)",
        "true": "开启"
      }
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh-tw",
    "yue": "yue",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt-br",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_mo": "mn",
    "mn_cy": "mn-cy",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa",
    "sv": "sv",
    "pl": "pl",
    "nl": "nl",
    "uk": "uk"
  }
}